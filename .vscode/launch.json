{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-Start<items>",
            "request": "launch",
            "mainClass": "org.oddcyb.items.Start",
            "projectName": "items",
            "args": "--mapdb --mapdb-file ${workspaceFolder}/var/items.db --mapdb-map items"
        }
    ]
}